{
  "name": "server",
  "author": "Charles De Lafontaine",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "tsc && concurrently --kill-others \"tsc -w \" \"nodemon out/server/app/www.js\" ",
    "lint": "tslint --project tsconfig.json \"./app/**/*.ts\" --format stylish",
    "test": "mocha",
    "coverage": "nyc mocha",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "clean": "rimraf out node_modules",
    "postinstall": "rimraf false"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "include": [
      "app/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.spec.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true,
    "cache": true,
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "@types/pg": "^8.6.5",
    "axios": "~0.27.2",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "eslint": "^8.24.0",
    "express": "^4.18.1",
    "inversify": "^6.0.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "pg": "^8.8.0",
    "pg-promise": "^10.12.0",
    "ps-tree": "^1.2.0",
    "reflect-metadata": "~0.1.13",
    "serve-favicon": "^2.5.0",
    "socket.io": "^4.5.2",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/express-serve-static-core": "^4.17.31",
    "@types/mocha": "^10.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.23",
    "@types/node-uuid": "~0.0.29",
    "@types/sinon": "^10.0.13",
    "chai": "^4.3.6",
    "concurrently": "^7.4.0",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^14.0.0",
    "typescript": "^4.8.4"
  }
}
